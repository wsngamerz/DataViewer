{
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/plugin-facebook-backend/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/libs/plugin-facebook-backend",
                "tsConfig": "libs/plugin-facebook-backend/tsconfig.lib.json",
                "packageJson": "libs/plugin-facebook-backend/package.json",
                "main": "libs/plugin-facebook-backend/src/index.ts",
                "assets": ["libs/plugin-facebook-backend/*.md"]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["libs/plugin-facebook-backend/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/libs/plugin-facebook-backend"],
            "options": {
                "jestConfig": "libs/plugin-facebook-backend/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}
